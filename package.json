{
  "name": "eccentric-event",
  "version": "6.1.1",
  "type": "module",
  "packageManager": "bun@1.2.5",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "cross-env NODE_OPTIONS='--trace-warnings' astro build",
    "postbuild": "pagefind --site dist",
    "preview": "astro preview",
    "lint:code": "biome check . && astro check --tsconfig tsconfig.json",
    "lint:md": "prettier --check **/*.md",
    "lint": "bun run lint:code && bun run lint:md",
    "fix:code": "biome check --write .",
    "fix:md": "prettier --write **/*.{md,mdx}",
    "fix": "bun run fix:code && bun run fix:md"
  },
  "dependencies": {
    "@astrojs/markdown-remark": "^6.3.1",
    "@astrojs/mdx": "^4.2.3",
    "@astrojs/rss": "^4.0.11",
    "@astrojs/sitemap": "^3.3.0",
    "@astrojs/tailwind": "^6.0.2",
    "@astrojs/vercel": "^8.1.3",
    "@fontsource/ia-writer-mono": "^5.2.5",
    "@tailwindcss/vite": "4.0.0-beta.10",
    "@vercel/analytics": "^1.5.0",
    "astro": "^5.6.1",
    "astro-expressive-code": "^0.39.0",
    "astro-icon": "^1.1.5",
    "astro-robots-txt": "^1.0.0",
    "astro-webmanifest": "^1.0.0",
    "autocorrect": "^1.2.0",
    "cssnano": "^7.0.6",
    "hastscript": "^9.0.1",
    "mdast-util-directive": "^3.1.0",
    "mdast-util-to-markdown": "^2.1.2",
    "mdast-util-to-string": "^4.0.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-external-links": "^3.0.0",
    "rehype-katex": "^7.0.1",
    "rehype-unwrap-images": "^1.0.0",
    "remark-directive": "^3.0.1",
    "remark-math": "^6.0.0",
    "satori": "^0.12.2",
    "satori-html": "^0.3.2",
    "sharp": "^0.33.5",
    "turndown": "^7.2.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@biomejs/biome": "^1.9.4",
    "@iconify-json/fa6-brands": "^1.2.5",
    "@iconify-json/material-symbols-light": "^1.2.19",
    "@iconify-json/mdi": "^1.2.3",
