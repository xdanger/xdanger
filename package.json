    "@resvg/resvg-js": "^2.6.2",
    "@tailwindcss/typography": "^0.5.16",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "glob": "^11.0.1",
    "pagefind": "^1.3.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "reading-time": "^1.5.0",
    "tailwindcss": "^4.1.3",
    "typescript": "^5.8.3"
  }
